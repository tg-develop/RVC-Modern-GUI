  "dependencies": {
    "@dannadori/voice-changer-client-js": "1.0.180",
    "@dannadori/worker-manager": "^1.0.20",
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/sortable": "7.0.2",
    "@dnd-kit/utilities": "3.2.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
